  title={Personalized estimates of brain structural variability in individuals with early psychosis},
  author={Antoniades, Mathilde and Haas, Shalaila S and Modabbernia, Amirhossein and Bykowsky, Oleg and Frangou, Sophia and Borgwardt, Stefan and Schmidt, Andr{\'e}},
  journal={Schizophrenia bulletin},
  volume={47},
